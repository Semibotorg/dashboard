{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es6",
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "noImplicitAny": true,
    "declaration": false,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "esModuleInterop":true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}